{
    "name": "prs-atm",
    "description": "A CLI client for PRESS.one .",
    "version": "2.2.1",
    "private": false,
    "homepage": "https://github.com/Press-One/prs-atm",
    "main": "index.js",
    "bin": {
        "prs-atm": "bin/prs-atm.js"
    },
    "engines": {
        "node": ">=10.x"
    },
    "scripts": {
        "start": "node index.js",
        "debug": "node --inspect --trace-warnings debug.js",
        "test": "node --inspect --trace-warnings test.js",
        "updep": "npx ncu -u && npm install && ( git commit -am 'update dependencies' || true )",
        "gitsync": "git pull && git push",
        "prepublishOnly": "npm run readme && npm version patch && npm run updep && npm run gitsync",
        "readme": "cat template.md > README.md && echo '\n```markdown' >> README.md && ./bin/prs-atm.js >> README.md && echo '```' >> README.md && ( git commit -am 'update README.md' || true )",
        "docker-build-hub": "docker build --no-cache . -t pressone/prs-atm && docker push pressone/prs-atm",
        "docker-build-cn": "docker build --no-cache . -t dockerhub.qingcloud.com/pressone/prs-atm && docker push dockerhub.qingcloud.com/pressone/prs-atm",
        "docker-build": "npm run docker-build-hub && npm run docker-build-cn"
    },
    "author": "Leask Wong <i@leaskh.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Press-One/prs-atm.git"
    },
    "dependencies": {
        "blessed": "^0.1.81",
        "blessed-contrib": "^4.8.20",
        "eosjs-keygen": "^1.3.2",
        "keythereum-pure-js": "^1.1.9",
        "prs-utility": "^0.1.0",
        "readline-sync": "^1.4.10",
        "sushitrain": "^1989.7.49",
        "table": "^6.0.3",
        "yargs": "^16.1.1"
    },
    "devDependencies": {
        "@nodeutils/defaults-deep": "^1.1.0",
        "aes-js": "^3.1.2",
        "bignumber.js": "^9.0.1",
        "cids": "^1.0.2",
        "eosjs-ecc": "^4.0.7",
        "ipfs": "^0.52.1",
        "ipfs-bitswap": "^4.0.0",
        "ipfs-repo": "^7.0.0",
        "libp2p": "^0.29.3",
        "libp2p-kad-dht": "^0.20.1",
        "libp2p-mplex": "^0.10.1",
        "libp2p-secio": "^0.13.1",
        "libp2p-tcp": "^0.15.1",
        "mime-types": "^2.1.27",
        "multihashing": "^0.3.3",
        "ncp": "^2.0.0",
        "peer-id": "^0.14.2",
        "peer-info": "^0.17.5",
        "promisify-es6": "^1.0.3",
        "rimraf": "^3.0.2"
    }
}
